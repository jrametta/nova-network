[SWITCH]
username = <%= @brocade_vdx_username %>
password = <%= @brocade_vdx_password %>  
address  = <%= @brocade_vdx_ipaddress %>
ostype   = NOS 

[DATABASE]
sql_connection = mysql://<%= @db_user %>:<%= @db_password %>@<%= @db_ip_address %>/<%= @db_name %>

[PHYSICAL_INTERFACE]
physical_interface = <%= @physical_interfaces %>

[VLANS]
network_vlan_ranges = <%= @network_vlan_ranges %>

[AGENT]
root_helper = sudo /usr/bin/quantum-rootwrap /etc/quantum/rootwrap.conf

[LINUX_BRIDGE]
physical_interface_mappings = <%= @interface_mappings %>
