[SWITCH]
username = <%= @brocade_vdx_username %>
password = <%= @brocade_vdx_password %>  
address  = <%= @brocade_vdx_ipaddress %>
ostype   = NOS 

[DATABASE]
sql_connection = mysql://<%= @db_user %>:<%= @db_password %>@<%= @db_ip_address %>/<%= @db_name %>

[PHYSICAL_INTERFACE]
physical_interface = physnet1

[VLANS]
network_vlan_ranges = physnet1:2:2000

[AGENT]
root_helper = sudo /usr/local/bin/quantum-rootwrap /etc/quantum/rootwrap.conf

[LINUX_BRIDGE]
physical_interface_mappings = physnet1:eth2
