[VLANS]
# Default: network_vlan_ranges =
tenant_network_type = vlan
network_vlan_ranges = <%= @network_vlan_ranges %>

[DATABASE]
# Example:
# sql_connection = mysql://root:nova@127.0.0.1:3306/quantum_linux_bridge
sql_connection = mysql://<%= @db_user %>:<%= @db_password %>@<%= @db_ip_address %>/<%= @db_name %>
reconnect_interval = 2

[LINUX_BRIDGE]
# Default: physical_interface_mappings =
physical_interface_mappings = <%= @interface_mappings %>

[AGENT]
# Agent's polling interval in seconds
polling_interval = 2

[SECURITYGROUP]
# Firewall driver for realizing quantum security group function
firewall_driver = quantum.agent.linux.iptables_firewall.IptablesFirewallDriver
firewall_driver = <%= @ovs_firewall_driver %>

